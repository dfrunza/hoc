proc void print_str(char* str)
{
  while(*str != '\0')
  {
    putc(*str);
    str = str + (type char*)1;
  }
}

proc void putc(char c)
{
  asm
  {
    push_reg fp
    push_int -13
    add_int
    load 1
    putc
  }
}

proc void main()
{
  print_str(&"Hello World!");
}

main();

