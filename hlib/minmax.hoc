proc int max(int a, int b)
{
  var int result = a;
  if(a < b)
    result = b;
  return result;
}

proc int min(int a, int b)
{
  var int result = a;
  if(a > b)
    result = b;
  return result;
}

proc float fmax(float a, float b)
{
  var float result = a;
  if(a < b)
    result = b;
  return result;
}

proc float fmin(float a, float b)
{
  var float result = a;
  if(a > b)
    result = b;
  return result;
}
