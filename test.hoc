var int len;

proc int strlen(char* s)
{
  var int len = 0;
  while(*s != '\0')
  {
    s = s+1;
    len = len+1;
  }
  return len;
}

proc int main()
{
  var char* str = "abcdef";
  var char* str2 = "Hello World!";
  len = strlen(str);
  var int len2 = strlen(str2);
  len = len + len2;
  return len;
}


