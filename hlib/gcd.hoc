proc gcd_(var a, var b)
{
  var result = a;
  if(b != 0)
  {
    result = b;
    var r = a % b;
    if(r == 0)
      result = b;
    else
      result = gcd_(b, r);
  }
  return result;
}

/*
   Greatest Common Divisor
 */
proc gcd(var a, var b)
{
  return gcd_(abs(a), abs(b));
}

/*
   Least Common Multiple
 */
proc lcm(var a, var b)
{
  return abs(a*b)/gcd(a, b);
}


