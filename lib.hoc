proc max(var a, var b)
{
  var result = a;
  if(a < b)
    result = b;
  return result;
}

proc min(var a, var b)
{
  var result = a;
  if(a > b)
    result = b;
  return result;
}

proc power(var a, var power)
{
  var p = power;
  var result = 1;
  while(p)
  {
    result = result*a;
    p = p-1;
  }
  return result;
}
