void print_numbers([]int numbers, int len)
{
  int i; i = 0;
  while(i < len)
  {
    print_int(numbers[i]);
    print_str(&"\n");

    i = i+1;
  }
}

[]int make_numbers(int how_many)
{
  int i; i = 0;
  int c1; c1 = 17;
  int c2; c2 = 31;
  []int numbers;
  numbers = ([]int):alloc(how_many);

  while(i < how_many)
  {
    if((i mod 2) == 0)
      numbers[i] = c1 * i;
    else
      numbers[i] = c2 * i;

    i = i+1;
  }
  return numbers;
}

