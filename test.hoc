proc int strlen(char* s)
{
  var int len = 0;
  while(*s != '\0')
  {
    s = s+1;
    len = len+1;
  }
  return len;
}

proc int main()
{
  var char* str = "abc";
  *str = 'x';
  *(str+1) = 'y';
  *(str+2) = '\0';
  return strlen(str);
}


