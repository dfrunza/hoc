var int* numbers;
var char* some_message = "Hello World!!\n";

proc void print_numbers(int* numbers, int len)
{
  var int i = 0;
  while(i < len)
  {
    print_int(i);
    print_str("=");
    print_int(numbers[i]);
    print_str("\n");

    i = i+1;
  }
}

proc int* make_numbers(int how_many)
{
  var int i = 0;
  var int constant = 17;
  var int* numbers = new(int, how_many);

  while(i < how_many)
  {
    numbers[i] = constant * i;

    i = i+1;
  }
  return numbers;
}

proc int main()
{
  var int count = 20;
  numbers = make_numbers(count);
  print_numbers(numbers, count);

  print_str("Hi there!\n");
  fibo(40);
  print_str("-----------\n");
  fizzbuzz(50);
  return 0;
}

