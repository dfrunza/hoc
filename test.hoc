proc Foo(var i, var j)
{
  return i+j;
}

proc Boo(var i, var j)
{
  var c;
  c = 2;
  var r;
  r = 0;

  while(c)
  {
    var z;
    z = 2;
    while(z)
    {
      r = r + (i-j);
      z = z-1;
    }
    c = c-1;
  }
  return r;
}

proc Main()
{
  var a;
  var b;
  a = Foo(99, 88);
  b = Boo(99, 88);
  return a+b;
}
